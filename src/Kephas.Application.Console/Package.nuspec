        <file src="bin\$configuration$\net45\Kephas.Application.Console.xml" target="lib\net45" />
        <file src="bin\$configuration$\net461\Kephas.Application.Console.dll" target="lib\net461" />
        <file src="bin\$configuration$\net461\Kephas.Application.Console.pdb" target="lib\net461" />
        <file src="bin\$configuration$\net461\Kephas.Application.Console.xml" target="lib\net461" />
