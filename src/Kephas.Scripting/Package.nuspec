<?xml version="1.0"?>
<package >
    <metadata>
        <id>Kephas.Scripting</id>
        <version>$version$</version>
        <title>Kephas.Scripting</title>
        <authors>Kephas Software SRL</authors>
        <owners>Kephas Software SRL</owners>
        <description>
            Provides abstract multi-dimensional modeling artifacts like classifiers, value types, and other model elements.
            Check the documentation and the samples from https://github.com/quartz-software/kephas/wiki and https://github.com/quartz-software/kephas/tree/master/Samples.
            Kephas Framework ("stone" in aramaic) is a very ambitious project aiming to deliver a framework for an application ecosystem.
        </description>
        <releaseNotes>
        </releaseNotes>
        <summary>Provides the infrastructure for executing scripts.</summary>
        <language>en-US</language>
        <projectUrl>https://github.com/quartz-software/kephas</projectUrl>
        <iconUrl>http://kephas-framework.com/Media/Default/Logo/kephas.logo.transparent.png</iconUrl>
        <requireLicenseAcceptance>true</requireLicenseAcceptance>
        <licenseUrl>http://opensource.org/licenses/MIT</licenseUrl>
        <copyright>Copyright Kephas Software SRL 2010-2018</copyright>
        <dependencies>
            <dependency id="Kephas.Core" version="$refversion$" />
            <dependency id="System.ValueTuple" version="4.3.0" />
        </dependencies>
        <references>
            <reference file="Kephas.Scripting.dll" />
        </references>
        <tags>kephas framework scripting</tags>
    </metadata>
    <files>
        <file src="**\*.cs" target="src"/>
        <file src="bin\$configuration$\netstandard2.0\Kephas.Scripting.dll" target="lib\netstandard2.0" />
        <file src="bin\$configuration$\netstandard2.0\Kephas.Scripting.pdb" target="lib\netstandard2.0" />
        <file src="bin\$configuration$\netstandard2.0\Kephas.Scripting.xml" target="lib\netstandard2.0" />
        <file src="bin\$configuration$\net45\Kephas.Scripting.dll" target="lib\net45" />
