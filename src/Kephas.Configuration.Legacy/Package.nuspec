        <tags>kephas configuration legacy</tags>
        <repository type="github" url="https://github.com/kephas-software/kephas" />
        <dependencies>
            <group targetFramework=".NETFramework4.5">
                <dependency id="Kephas.Core" version="$refversion$" exclude="Build,Analyzers" />
                <dependency id="System.ValueTuple" version="4.3.0" exclude="Build,Analyzers" />
